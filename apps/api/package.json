{
  "name": "@elysia-on-vercel/api",
  "type": "module",
  "version": "1.0.50",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "tsc -b",
    "vercel-build": "cd ../../ && bunx turbo build --filter=@elysia-on-vercel/api... &&  echo \"vercel-build called\"",
    "clean": "rm -rf dist .tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysia-on-vercel/shared": "workspace:*",
    "elysia": "latest"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.9.3"
  }
}
